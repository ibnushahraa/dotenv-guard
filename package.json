{
  "name": "dotenv-guard-monorepo",
  "version": "1.2.0",
  "private": true,
  "description": "Monorepo for dotenv-guard packages",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "test": "npm run test --workspaces",
    "test:core": "npm test --workspace=packages/core",
    "test:vite-plugin": "npm test --workspace=packages/vite-plugin",
    "test:nuxt-module": "npm test --workspace=packages/nuxt-module",
    "prepublishOnly": "npm test"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "author": "Syarwani (https://github.com/ibnushahraa)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ibnushahraa/dotenv-guard.git"
  },
  "bugs": {
    "url": "https://github.com/ibnushahraa/dotenv-guard/issues"
  },
  "homepage": "https://github.com/ibnushahraa/dotenv-guard#readme",
  "devDependencies": {
    "jest": "^30.2.0"
  }
}
